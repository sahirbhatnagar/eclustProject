citHeader("To cite eclust in publications use:")

citEntry(entry = "Article",
  title        = "A model for interpretable high dimensional interactions with an environment variable",
  author       = personList(as.person("Sahir Bhatnagar"),
                   as.person("Yi Yang"),
                   as.person("Mathieu Blanchette"),
                   as.person("Celia M.T. Greenwood")),
  journal      = "",
  year         = "",
  volume       = "",
  number       = "",
  pages        = "",
  url          = "",

  textVersion  =
  paste("Bhatnagar S. et al.",
        "A model for interpretable high dimensional interactions with an environment variable",
        "In preparation.")
)
